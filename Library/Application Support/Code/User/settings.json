  "editor.renderLineHighlight": "none",
  "editor.lightbulb.enabled": "off",
  "editor.showFoldingControls": "never",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.overviewRulerBorder": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.fontFamily": "Liga SFMono Nerd Font",
  "terminal.integrated.fontFamily": "Liga SFMono Nerd Font",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "scm.diffDecorations": "none",
  "extensions.ignoreRecommendations": true,
  "terminal.integrated.defaultProfile.osx": "fish",
  "zenMode.fullScreen": false,
  "window.zoomLevel": 1.5,
  "editor.formatOnSave": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "editor.lineNumbers": "relative",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnSaveMode": "file",
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": false,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "workbench.editorAssociations": {
    "*.pdf": "latex-workshop-pdf-hook"
  },
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "[cpp]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
  },
  "terminal.integrated.enableMultiLinePasteWarning": "always",
  "python.terminal.activateEnvironment": false,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "editor.quickSuggestionsDelay": 0,
  "git.autofetch": false,
  "workbench.colorTheme": "Dark+ Black",
  "workbench.tree.renderIndentGuides": "onHover",
  "workbench.tree.indent": 10,
  "workbench.sideBar.location": "right",
  "workbench.activityBar.location": "hidden", //put to top when having UI problems
  "workbench.iconTheme": "material-icon-theme",
  "editor.hover.delay": 500,
  "workbench.hover.delay": 0,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "jupyter.enableExtendedPythonKernelCompletions": true,
  "jupyter.exportWithOutputEnabled": true,
  "editor.hover.hidingDelay": 10000,
  "editor.multiCursorModifier": "ctrlCmd",
  "workbench.editor.enablePreview": false,
  "breadcrumbs.enabled": false,
  "update.mode": "manual",
  "git.confirmSync": false,
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.autoImportCompletions": true,
  "terminal.integrated.defaultLocation": "editor",
  "python.analysis.nodeExecutable": "/opt/homebrew/bin/node",
  "workbench.statusBar.visible": false,
  "window.titleBarStyle": "native",
  "custom-ui-style.electron": {
    "titleBarStyle": "hiddenInset"
  }
