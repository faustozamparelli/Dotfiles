  "latex-workshop.latexmk.path": "/Users/faustozamparelli/Library/TinyTeX/bin/universal-darwin/latexmk",
  "latex-workshop.latex.tools": [
    {
      "name": "tinytex-pdflatex",
      "command": "/Users/faustozamparelli/Library/TinyTeX/bin/universal-darwin/pdflatex",
      "args": [
        "-synctex=0",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    },
    {
      "name": "tinytex-bibtex",
      "command": "/Users/faustozamparelli/Library/TinyTeX/bin/universal-darwin/bibtex",
      "args": ["%DOCFILE%"]
    },
    {
      "name": "tinytex-latexmk",
      "command": "/Users/faustozamparelli/Library/TinyTeX/bin/universal-darwin/latexmk",
      "args": ["-pdf", "-interaction=nonstopmode", "-synctex=0", "%DOC%"]
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "TinyTeX latexmk (auto BibTeX)",
      "tools": ["tinytex-latexmk"]
    },
    {
      "name": "TinyTeX pdflatex → bibtex → pdflatex × 2",
      "tools": [
        "tinytex-pdflatex",
        "tinytex-bibtex",
        "tinytex-pdflatex",
        "tinytex-pdflatex"
      ]
    },
    {
      "name": "TinyTeX pdflatex twice (no synctex)",
      "tools": ["tinytex-pdflatex", "tinytex-pdflatex"]
    }
  ],
  "latex-workshop.latex.autoBuild.run": "onFileChange",
  "latex-workshop.latex.recipe.default": "TinyTeX latexmk (auto BibTeX)",
  "latex-workshop.latex.build.forceRecipeUsage": true,
  "latex-workshop.latex.autoClean.run": "never",
  "latex-workshop.latex.outDir": "%DIR%",
}
